@model MVC_SYSTEM.Models.tbl_Pkjmast
@{
    ViewBag.Title = "WorkerDelete";
    //Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">@GlobalResEstate.titleDelete</h3>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="modal-body">

        <div class="form-horizontal">
            <span id="progress" class="text-center" style="display: none;">
                <img src="~/Asset/Images/wait.gif" alt="wait" />
            </span>
            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstate.hdrNoPkj</label>
                <div class="col-md-9">
                    @Html.EditorFor(model => model.fld_Nopkj, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstate.hdrName</label>
                <div class="col-md-9">
                    @Html.EditorFor(model => model.fld_Nama, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstate.hdrNoKp</label>
                <div class="col-md-9">
                    @Html.EditorFor(model => model.fld_Nokp, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-9 col-sm-offset-3">
                    <h3>
                        <span class="label label-danger">
                            @GlobalResEstate.msgDelete
                        </span>
                    </h3>
                </div>
            </div>
        </div>
    </div>


    <div class="modal-footer">
        <button class="btn btn-primary" data-dismiss="modal">@GlobalResEstate.btnNo</button>
        <input class="btn btn-danger" type="submit" value=@GlobalResEstate.btnYes />
    </div>
}
