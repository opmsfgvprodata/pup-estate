
@{
    ViewBag.Title = "Socso";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<section class="content-wrapper main-content clear-fix">
    @using (Html.BeginForm("Socso", null, FormMethod.Get))
    {
        <div id="DataTable" class="well">
            <div class="modal-body">
                <div class="panel panel-primary">
                    <div class="panel-heading">@GlobalResEstate.lblSocso</div>
                    <div class="panel-body">
                        @Html.Action("SocsoDetail", "TableInfo")
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <a style="text-align:right" href="@Url.Action("Index", "TableInfo")" class="btn btn-warning">@GlobalResEstate.btnBack</a>
            </div>
        </div>
    }
</section>


