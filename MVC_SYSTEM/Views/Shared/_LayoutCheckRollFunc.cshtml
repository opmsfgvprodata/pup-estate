@using MVC_SYSTEM.Class
@{
    GetConfig getconfig = new GetConfig();
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=EDGE" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title - @getconfig.GetData("sysname")</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/Modernizrjquery")
    @Styles.Render("~/Content/jqueryui")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/MyCustomeJs")
    @Styles.Render("~/Content/DateTimePickerCSS2")
    @Scripts.Render("~/bundles/DateTimePickerJS2")
    @Scripts.Render("~/bundles/CheckingJS")

    <link href="~/Asset/Flag/css/flag-icon.css" rel="stylesheet" />
    <link href="~/Asset/Content/font-awesome.min.css" rel="stylesheet" />
</head>
<body>
    <div class="maincontent2 animated fadeInRight">
        @RenderBody()
    </div>
</body>
@Scripts.Render("~/bundles/MyCustomeJs")
@RenderSection("scripts", required: false)
</html>